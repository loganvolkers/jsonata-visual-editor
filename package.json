  "files": [
    "dist"
  ],
  "dependencies": {
    "jsonata": "1.7.0",
    "jsonata-ui-core": "^1.7.12"
  },
  "peerDependencies": {
    "react": ">=16.8.6"
  },
  "devDependencies": {
    "@types/json-schema": "7.0.3",
    "@types/react": "^16.14.46",
    "npm-run-all": "^4.1.5",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "3.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "scripts": {
    "start": "run-p dev:*",
    "dev": "run-p dev:*",
    "dev:tests": "vitest --watch",
    "build": "tsup src/index.ts --dts",
